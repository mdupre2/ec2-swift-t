# The region where the instances run 
export SWIFT_EC2_REGION=

# The path to the .pem file that is used for all instances 
export SWIFT_EC2_KEY=

# The name of the key. For example if the key is my_key.pem then this should be set to my_key
export SWIFT_EC2_KEY_NAME=

# The user of the ec2 instances. Change if not using ubuntu.
export SWIFT_EC2_USER=ubuntu

# The full path of the turbine program on the ec2 instances unless the directory of the turbine program is added to $PATH variable in ~/.profile
export SWIFT_EC2_TURBINE_PATH=

# The AWS Access Key ID
export AWS_ACCESS_KEY=

# The AWS Secret Access Key
export AWS_SECRET_KEY=

# The ec2 api home. This can be left alone.
export EC2_HOME=$(pwd)/ec2-api-tools-1.7.5-2.0

# The java home. If on a mac, leave it as it is 
export JAVA_HOME=$(/usr/libexec/java_home)


